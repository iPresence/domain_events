{
    "name": "ipresence/domain_events",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "psr/log": "^1.0",
        "ramsey/uuid": "^3.8",
        "ipresence/monitoring": "^1.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^5.1",
        "php-amqplib/php-amqplib": "^2.6",
        "behat/behat": "^3.3",
        "symfony/dependency-injection": "^4.2",
        "symfony/config": "^4.2",
        "symfony/http-kernel": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "IPresence\\DomainEvents\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\IPresence\\DomainEvents\\": "tests/spec/IPresence/DomainEvents/",
            "behat\\IPresence\\DomainEvents\\": "./tests/behat/bootstrap"
        }
    }
}
